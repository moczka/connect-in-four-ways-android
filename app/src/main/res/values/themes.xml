<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Custom Themes -->s
    <style name="Theme.Classic" parent="Theme.AppCompat.DayNight.NoActionBar">
        <item name="colorPrimary">@color/primary_background</item>
        <item name="colorSecondary">@color/primary_over_background</item>
        <item name="android:buttonStyle">@style/Theme.Classic.ButtonStyles</item>
    </style>
    <!-- Theme overrides -->
    <style name="Theme.Classic.ButtonStyles" parent="Widget.AppCompat.Button">
        <item name="android:background">@color/primary_over_background</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textAllCaps">false</item>
    </style>
</resources>